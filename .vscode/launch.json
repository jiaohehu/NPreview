  "version": "0.2.0",
  "configurations": [
    {
      "type": "node",
      "request": "launch",
      "name": "Launch HTML",
      "skipFiles": [
        "<node_internals>/**"
      ],
      "cwd": "${workspaceRoot}/tests",
      "program": "${workspaceFolder}/tests/html.js",
      "args":["a.md"]
    },
    {
      "type": "node",
      "request": "launch",
      "name": "Launch CONTEX",
      "skipFiles": [
        "<node_internals>/**"
      ],
      "cwd": "${workspaceRoot}/tests",
      "program": "${workspaceFolder}/tests/contex.js",
      "args": [
        "a.md"
      ]
    },
    {
      "type": "node",
      "request": "launch",
      "name": "Launch LATEX",
      "skipFiles": [
        "<node_internals>/**"
      ],
      "cwd": "${workspaceRoot}/tests",
      "program": "${workspaceFolder}/tests/latex.js",
      "args": [
        "a.md"
      ]
    }
  ]
}
